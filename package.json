{
  "name": "bookmark-action",
  "version": "3.1.2",
  "description": "Save website metadata to a YAML file.",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "eslint . --ext .js,.ts",
    "test": "jest --coverage",
    "build": "ncc build src/index.ts",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install"
  },
  "author": "Katy DeCorah",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.2",
    "@actions/github": "^5.0.3",
    "js-yaml": "^4.1.0",
    "open-graph-scraper": "^4.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/jest": "^28.1.4",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.38",
    "@types/open-graph-scraper": "^4.8.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.4.6",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.7.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/bookmark-action.git"
  },
  "bugs": {
    "url": "https://github.com/katydecorah/bookmark-action/issues"
  },
  "homepage": "https://github.com/katydecorah/bookmark-action#readme",
  "jest": {
    "resetMocks": true,
    "moduleNameMapper": {
      "^./utils.js$": "<rootDir>/src/utils.ts"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
