{
  "name": "bookmark-action",
  "version": "3.0.0",
  "description": "Save website metadata to a YAML file.",
  "main": "dist/index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "jest --coverage",
    "build": "ncc build src/index.ts",
    "pretty-quick": "pretty-quick",
    "prepare": "husky install"
  },
  "author": "Katy DeCorah",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "js-yaml": "^4.1.0",
    "open-graph-scraper": "^4.11.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@octokit/webhooks-definitions": "^3.67.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.8",
    "@types/open-graph-scraper": "^4.8.1",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/katydecorah/bookmark-action.git"
  },
  "bugs": {
    "url": "https://github.com/katydecorah/bookmark-action/issues"
  },
  "homepage": "https://github.com/katydecorah/bookmark-action#readme",
  "jest": {
    "resetMocks": true,
    "moduleNameMapper": {
      "^./utils.js$": "<rootDir>/src/utils.ts"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
